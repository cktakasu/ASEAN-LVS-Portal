{
  "name": "asean-lvs-business-portal",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 8080 --strictPort",
    "build": "tsc --noEmit && vite build",
    "build:local": "tsc --noEmit && VITE_LOCAL_PREVIEW=true vite build",
    "preview": "vite preview --host 0.0.0.0 --port 8080 --strictPort",
    "preview:local": "VITE_LOCAL_PREVIEW=true vite preview --host 0.0.0.0 --port 8080 --strictPort",
    "test": "vitest run"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.13.0",
    "recharts": "^3.7.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.3.4",
    "jsdom": "^27.0.0",
    "typescript": "^5.7.3",
    "vite": "^6.0.11",
    "vitest": "^3.2.4"
  }
}
